#include <stdio.h> 
#include <stdlib.h> 
#include <time.h>

int main() {
    int n;
    scanf("%d", &n);
    srand(1);
    FILE *fp;
    fp = fopen("lotto.txt", "w");
    fprintf(fp, "========= lotto649 =========\n");
    for (int i = 0; i < 5; i++) {
        fprintf(fp, "[%d]: ", i + 1);
        if (i < n) {
            int lotto[7];
            for (int j = 0; j < 7; ) {
                int num = rand() % 69 + 1;
                int dup = 0;
                for (int k = 0; k < j; k++) {
                    if (lotto[k] == num) {
                        dup = 1;
                        break;
                    }
                }
                if (dup == 0) {
                    lotto[j] = num;
                    fprintf(fp, "%02d ", num);
                    j++;
                }
            }
        } else {
                fprintf(fp, "__ __ __ __ __ __ __");
            }
            fprintf(fp,"\n");
        }
    fprintf(fp, "========= csie@CGU =========\n");
    fclose(fp);
    return 0;
}         
