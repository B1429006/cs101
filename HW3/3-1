#include <stdio.h>
#include <stdlib.h>

int main(void) {
    int N;
    if (scanf("%d", &N) != 1) return 0;

    FILE *fp = fopen("lotto.txt", "w");
    if (!fp) return 0;

    srand(1);  

    fprintf(fp, "========= lotto649 =========\n");

    for (int i = 1; i <= 5; i++) {
        fprintf(fp, "[%d]: ", i);
        if (i <= N) {
            int used[70] = {0}; 
            int cnt = 0;
            while (cnt < 7) {
                int r = rand() % 69 + 1; 
                if (!used[r]) {
                    used[r] = 1;
                    if (cnt > 0) fprintf(fp, " ");
                    fprintf(fp, "%02d", r); 
                    cnt++;
                }
            }
            fprintf(fp, "\n");
        } else {
            fprintf(fp, "__ __ __ __ __ __ __\n");
        }
    }

    fprintf(fp, "========= csie@CGU =========\n");
    fclose(fp);
    return 0;
}
